<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.spring.mybatis.mapper.SysUserMapper">

    <select id="findUserById" resultType="com.spring.mybatis.model.SysUser">
        select * from t_sys_user u where u.id=#{id}
    </select>

</mapper>